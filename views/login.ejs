<%- include('partials/introheader') %>

  <div class="containerbox">
    <img src="/images/d1.jpg" alt="diary" class="img1">
    <h1 class="intro_name">Log in</h1>
    <form action="/login" method="POST">
      <div class="form-group">
        <label for="email">Email : </label>
        <input type="email" class="form-control" name="username" autocomplete="off" required>
      </div>
      <div class="form-group">
        <label for="password">Password : </label>
        <input type="password" class="form-control" name="password" required>
      </div>
      <a href="/" class="btn btn-dark">Back</a>
      <button type="submit" class="btn btn-dark">Login</button>
    </form>

  </div>









<%- include('partials/introfooter') %>
